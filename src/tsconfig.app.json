{
  "compilerOptions": {
    "sourceMap": true,
    "stripInternal": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "lib": [
      "es2016",
      "dom"
      ],
    "outDir": "../dist",
    "target": "es5",
    "module": "es2015",
    "baseUrl": "",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "node_modules/*"
  ],
  "angularCompilerOptions": {
    "strictMetadataEmit": true
  }
}
